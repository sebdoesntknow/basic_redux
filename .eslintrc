{
  "parserOptions": {
    "ecmaVersion": 7,
    "ecmaFeatures": {
      "impliedStrict": true,
      "experimentalObjectRestSpread": true
    },
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "env": {
    "es6": true
  },
  "plugins": [
    "react",
  ],
  "extends": [
    "eslint:recommended",
  ],
  "rules": {
    "eqeqeq": 2,
  },
  "settings": {
    "flowtype": {
      "onlyFilesWithFlowAnnotation": false
    }
  }
}
